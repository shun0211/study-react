{
  "name": "study-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-p --continue-on-error lint:*",
    "lint:eslint": "eslint --ext .jsx,.js .",
    "lint:prettier": "prettier --check .",
